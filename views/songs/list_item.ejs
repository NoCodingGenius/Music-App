<li class="collection-item">
  <div class="row">
    <div class="col s12 m5 l5">
      <%= song.title %>
    </div>
    <div class="col s12 m2 l2">
      <a class="black-text" href="/artists/<%= song.artistId %>"><u><%= song.artistName %></u></a>
    </div>
    <div class="col s12 m3 l3">
      <a class="black-text" href="/albums/<%= song.album_id %>"><u><%= song.albumName %></u></a>
    </div>
    <div class="col s12 m2 l2 center-align">
      <%= song.trackTime %>
    </div>
  </div>
</li>
