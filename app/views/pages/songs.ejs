<% include('../shared/header', {'pageTitle' : 'Songs'}); %>
  <h1 class="white-text">Songs</h1>
  <div class="divider"></div>
  <div class="row"></div>
  <% songs.forEach(function(songs){%>
    <div class="row">
      <div class="col s12 m3 l3">
        <ul class="collection with-header">
          <li class="collection-header white-text grey">
            <h4>Song</h4></li>
          <li class="collection-item">
            <%= songs.title %>
          </li>
        </ul>
      </div>

      <div class="col s12 m3 l3">
        <ul class="collection with-header">
          <li class="collection-header white-text grey">
            <h4>Artist</h4></li>
          <li class="collection-item">
            <%= artists.name %>
          </li>
        </ul>
      </div>

      <div class="col s12 m3 l3">
        <ul class="collection with-header">
          <li class="collection-header white-text grey">
            <h4>Album</h4></li>
          <li class="collection-item">
            <%= albums.title %>
          </li>
        </ul>
      </div>

      <div class="col s12 m3 l3">
        <ul class="collection with-header">
          <li class="collection-header white-text grey">
            <h4>Length</h4></li>
          <li class="collection-item">
            <%= songs.length %>
          </li>
        </ul>
      </div>

    </div>
    <%})%>
      <% include ../shared/footer %>
